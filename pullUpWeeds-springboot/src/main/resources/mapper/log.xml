        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.chl.pullUpWeeds.mapper.LogMapper">


    <insert id="addLog">
        insert into log(username,content,log_time) values (#{username},#{content},#{time})
    </insert>



</mapper>