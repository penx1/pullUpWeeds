\"30%\" :before-close=\"handleClose\"\r\n        >\r\n          <span>   购物车无商品，请添加商品。</span><br>\r\n\r\n          <br>\r\n          <span slot=\"footer\" class=\"dialog-footer\">\r\n\r\n    <el-button type=\"primary\" @click= toStore>确 定</el-button>\r\n  </span>\r\n        </el-dialog>\r\n\r\n      </div>\r\n\r\n\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// @ is an alias to /src\r\n\r\n\r\nimport request from \"../utils/request\";\r\n\r\nexport default {\r\n  data() {\r\n    return{\r\n\r\n      fits: ['fill', 'contain', 'cover', 'none', 'scale-down'],\r\n      url: require('../er.jpg'),\r\n      pri:0,\r\n      money:0,\r\n      num:0,\r\n      currentPage:1,\r\n      pageSize: 10,\r\n      eroVisible:false,\r\n      dialogVisible: false,\r\n      form: {\r\n        name: 233\r\n      },\r\n      total:0,\r\n\r\n      search: '',\r\n      tableData: [\r\n\r\n      ]\r\n    }\r\n  },\r\n  name: 'Cart',\r\n  components: {\r\n\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n\r\n  methods:{\r\n    toStore(){\r\n      // this.$router.go(-1);\r\n      this.$router.replace(\"/store\")\r\n    },\r\n    handleClose(done) {\r\n      this.$confirm('是否退出')\r\n          .then(_ => {\r\n            done();\r\n          })\r\n          .catch(_ => {});\r\n    },\r\n    price(){\r\n      let n = 0\r\n      this.tableData.filter((data)=>{\r\n\r\n        if (data.num!=null){\r\n          n=data.num*data.price+n\r\n          console.log(n)\r\n        }\r\n      })\r\n\r\n      this.pri=n\r\n    },\r\n    filesUploadSuccess(res){\r\n      this.form.cover=res.data\r\n    },\r\n    load(){\r\n      let goodss =sessionStorage.getItem('goods')\r\n      console.log(goodss)\r\n\r\n\r\n      if (goodss !== null){\r\n        goodss = goodss.slice(1,goodss.length-1)\r\n        request.get(\"/store/cart\",{\r\n          // get请求需要params\r\n          params:{\r\n            pageNum: this.currentPage,\r\n            pageSize: this.pageSize,\r\n            search: this.search,\r\n            goods:goodss\r\n          }\r\n        }).then(res =>{\r\n          this.tableData = res.data.records\r\n          this.total = res.data.total\r\n        })\r\n\r\n      }else {\r\n        this.eroVisible = true\r\n      }\r\n\r\n    },\r\n\r\n    add(){\r\n      this.dialogVisible = true\r\n      this.form={}\r\n      this.$refs['upload'].clearFiles()\r\n    },\r\n    save: function () {\r\n      if (this.form.id){//  更新\r\n        request.put(\"/store\",this.form).then(res=> {\r\n          console.log(res)\r\n          if (res.code === '0'){\r\n            this.$message({\r\n              type:\"success\",\r\n              message:\"更新成功\"\r\n            })\r\n          }else {\r\n            this.$message({\r\n              type:\"error\",\r\n              message:res.msg\r\n            })\r\n          }\r\n          this.load()\r\n          this.dialogVisible=false\r\n        })\r\n        this.dialogVisible = false\r\n      }else {//  新增\r\n        request.post(\"/store\",this.form).then(res=> {\r\n          console.log(res)\r\n          if (res.code === '0'){\r\n            this.$message({\r\n              type:\"success\",\r\n              message:\"新增成功\"\r\n            })\r\n          }else {\r\n            this.$message({\r\n              type:\"error\",\r\n              message:res.msg\r\n            })\r\n          }\r\n          this.load()\r\n          this.dialogVisible=false\r\n        })\r\n\r\n      }\r\n\r\n\r\n    },\r\n    handleEdit(row){\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogVisible = true\r\n    },\r\n    handleDelete(id){\r\n      request.delete(\"/store/\"+id).then(res=>{\r\n        if (res.code === '0'){\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"删除成功\"\r\n          })\r\n        }else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n        this.load()\r\n      })\r\n    },\r\n    handleSizeChange(pageSize){//改变当前页数触发\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum){//改变当前页码触发\r\n      this.currentPage = pageNum\r\n      this.load()\r\n    },\r\n\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n"]}]}