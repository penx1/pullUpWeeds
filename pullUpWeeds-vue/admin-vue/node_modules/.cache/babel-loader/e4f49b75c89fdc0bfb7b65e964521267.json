AAI;AACjB,YAAI,CAAC,MAAL,EAAY;AACN,UAAA,MAAK,GAAI,IAAI,CAAJ,CAAM,cAAN,CAAT;AACA,UAAA,MAAM,CAAC,MAAP,CAAc,YAAd,GAA6B,CAC3B,UAD2B,EAE3B,OAF2B,EAG3B,OAH2B,CAA7B;AAKA,UAAA,MAAM,CAAC,MAAP;AACP,SARC,MAQI;AACJ,UAAA,MAAM,CAAC,GAAP,CAAW,IAAX,CAAgB,EAAhB;AACF;;AACA,QAAA,MAAM,CAAC,GAAP,CAAW,IAAX,CAAgB,GAAG,CAAC,OAApB;AACA,OAbA;AAeD,KAtGK;AAuGN,IAAA,YAvGM,wBAuGO,EAvGP,EAuGU;AAAA;;AACd,MAAA,OAAO,CAAC,MAAR,CAAe,cAAY,EAA3B,EAA+B,IAA/B,CAAoC,UAAA,GAAG,EAAE;AACvC,QAAA,MAAI,CAAC,OAAL,CAAa,GAAb;;AACA,QAAA,MAAI,CAAC,IAAL;AACD,OAHD;AAID,KA5GK;AA6GN,IAAA,gBA7GM,4BA6GW,QA7GX,EA6GoB;AAAC;AACzB,WAAK,QAAL,GAAgB,QAAhB;AACA,WAAK,IAAL;AACD,KAhHK;AAiHN,IAAA,mBAjHM,+BAiHc,OAjHd,EAiHsB;AAAC;AAC3B,WAAK,WAAL,GAAmB,OAAnB;AACA,WAAK,IAAL;AACD;AApHK;AA3BK,CAAf","sourcesContent":["<template >\n  <div >\n    <div style=\"padding: 10px\">\n      <div style=\"margin: 10px 0\">\n<!--        <el-button type=\"primary\" @click=\"add\">新增</el-button>-->\n\n      </div>\n      <div  style=\"margin: 10px 0\">\n        <el-input v-model=\"search\" placeholder=\"输入关键字\" style=\"width: 20%\" clearable></el-input>\n        <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"load\">查询</el-button>\n      </div>\n      <el-table :data=\"tableData\" border  stripe style=\"width: 100%\">\n        <el-table-column prop=\"id\" label=\"ID\" sortable=\"\"> </el-table-column>\n<!--        <el-table-column prop=\"content\" label=\"内容\" sortable=\"\"></el-table-column>-->\n        <el-table-column prop=\"user.nickname\" label=\"作者\" sortable=\"\"></el-table-column>\n        <el-table-column prop=\"articleId\" label=\"文章id\" sortable=\"\"></el-table-column>\n        <el-table-column prop=\"time\" label=\"发表时间\" sortable=\"\"></el-table-column>\n        <el-table-column prop=\"floor\" label=\"楼层\" sortable=\"\"> </el-table-column>\n\n\n        <el-table-column fixed=\"right\" label=\"操作\">\n          <template #default=\"scope\">\n            <el-button size=\"mini\" type=\"success\" @click=\"handleContent(scope.row)\" >内容</el-button>\n            <el-button size=\"mini\" @click=\"handleEdit(scope.row)\" >编辑</el-button>\n            <el-popconfirm title=\"这是一段内容确定删除吗？\" @confirm=\"handleDelete(scope.row.id)\">\n              <template #reference>\n                <el-button size=\"mini\"   type=\"danger\" >删除</el-button>\n              </template>\n            </el-popconfirm>\n          </template>\n        </el-table-column>\n\n      </el-table>\n      <div style=\"margin: 10px\">\n        <el-pagination\n            @size-change=\"handleSizeChange\"\n            @current-change=\"handleCurrentChange\"\n            :current-page=\"currentPage\"\n            :page-sizes=\"[5,10,20]\"\n            :page-size=\"pageSize\"\n            layout=\"total, sizes, prev, pager, next, jumper\"\n            :total=\"total\"\n        >\n        </el-pagination>\n        <el-dialog title=\"修改评论\" v-model=\"dialogVisible\" width=\"60%\">\n          <el-form  :model=\"form\" label-width=\"120px\">\n\n\n<!--            <el-form-item label=\"作者\">-->\n<!--              <el-input v-model=\"form.authorId\" style=\"width: 80%;\"></el-input>-->\n<!--            </el-form-item>-->\n\n<!--            wangeditor-->\n            <div id=\"wangeditor1\">\n\n            </div>\n          </el-form>\n          <span slot=\"footer\" class=\"dialog-footer\">\n              <el-button @click=\"dialogVisible = false\">取 消</el-button>\n              <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n            </span>\n        </el-dialog>\n\n\n        <el-dialog title=\"文章内容\" v-model=\"vcontent\" width=\"50%\">\n          <el-card>\n            <div v-html=\"contents.content\"></div>\n          </el-card>\n        </el-dialog>\n      </div>\n\n\n    </div>\n\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\n\nimport E from 'wangeditor'\nimport request from \"../utils/request\";\nlet editor\nexport default {\n  data() {\n    return{\n      currentPage:1,\n      pageSize: 5,\n      dialogVisible: false,\n      form: {\n\n      },\n      total:0,\n      contents:{},\n      vcontent:false,\n      search: '',\n      tableData: [\n\n      ]\n    }\n  },\n  name: 'Comment',\n  components: {\n\n  },\n\n  created() {\n    this.load()\n  },\n\n  methods:{\n    // filesUploadSuccess(res){\n    //   this.form.avatar=res.data\n    //   this.$refs.upload.clearFiles()\n    // },\n    loadMsg(res){\n      if (res.flag === true){\n        this.$message({\n          type:\"success\",\n          message:res.message\n        })\n      }else {\n        this.$message({\n          type:\"error\",\n          message:res.message\n        })\n      }\n    },\n    load(){\n      console.log(\"session\"+sessionStorage.getItem(\"user\"));\n\n\n      request.get(\"/comment\",{\n        // get请求需要params\n        params:{\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res =>{\n        this.loadMsg(res)\n        this.tableData = res.data.records\n        this.total = res.data.total\n      })\n\n\n    },\n    add(){\n\n      this.dialogVisible = true\n      this.form={}\n      this.$nextTick(()=>{  //对未来元素的操作\n       if (!editor){\n         editor = new E('#wangeditor1')//创建wangeditor对象\n         editor.config.uploadImgServer = \"http://localhost:8090/photo/upload\"//配置图片上传地址\n         editor.config.uploadFileName = \"file\"\n         editor.create()\n        }else {//如果已有editor对象则清空其中的内容\n        editor.txt.html(\"\")\n        }\n      }\n    )\n    },\n    save: function () {\n      if (this.form.id){//  更新\n        this.form.content=editor.txt.html()\n        request.put(\"/comment\",this.form).then(res=> {\n\n          this.loadMsg(res)\n\n          this.load()\n          this.dialogVisible=false\n        })\n        this.dialogVisible = false\n      }else {//  新增\n        this.form.content=editor.txt.html()\n        let userStr= sessionStorage.getItem(\"user\") || \"{}\"\n        let user = JSON.parse(userStr)\n        this.form.authorId = user.id\n        request.post(\"/comment\",this.form).then(res=> {\n          this.loadMsg(res)\n\n          this.load()\n          this.dialogVisible=false\n        })\n\n      }\n\n\n    },\n    handleContent(row){\n      this.contents=row\n      this.vcontent=true\n    },\n    handleEdit(row){\n      this.form = JSON.parse(JSON.stringify(row))\n      this.dialogVisible = true\n      this.$nextTick(()=>{\n        if (!editor){\n              editor = new E('#wangeditor1')\n              editor.config.excludeMenus = [\n                'emoticon',\n                'video',\n                'image'\n              ]\n              editor.create()\n      }else {\n        editor.txt.html(\"\")\n      }\n      editor.txt.html(row.content)\n      }\n     )\n    },\n    handleDelete(id){\n      request.delete(\"/comment/\"+id).then(res=>{\n        this.loadMsg(res)\n        this.load()\n      })\n    },\n    handleSizeChange(pageSize){//改变当前页数触发\n      this.pageSize = pageSize\n      this.load()\n    },\n    handleCurrentChange(pageNum){//改变当前页码触发\n      this.currentPage = pageNum\n      this.load()\n    },\n\n\n  }\n}\n</script>\n"],"sourceRoot":""}]}