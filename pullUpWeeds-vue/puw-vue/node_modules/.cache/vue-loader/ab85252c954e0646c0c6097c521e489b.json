           @keyup.enter.native=\"login\"\n          />\n        </el-form-item>\n      </el-form>\n      <!-- 登录按钮 -->\n      <el-button type=\"primary\" @click=\"login\">登录</el-button>\n    </div>\n  </div>\n</template>\n<script>\nimport request from \"@/utils/request\";\nimport axios from \"axios\";\n\nexport default {\n  data: function() {\n    return {\n      loginForm: {\n        username: \"\",\n        password: \"\"\n      },\n      rules: {\n        username: [\n          { required: true, message: \"用户名不能为空\", trigger: \"blur\" }\n        ],\n        password: [{ required: true, message: \"密码不能为空\", trigger: \"blur\" }]\n      }\n    };\n  },\n  methods: {\n    login() {\n      this.$refs.ruleForm.validate(valid => {\n        if (valid) {\n          const that = this;\n          // eslint-disable-next-line no-undef\n\n          var captcha = new   TencentCaptcha(\n            // this.config.TENCENT_CAPTCHA,\n              \"2068422883\",\n            function(res) {\n              if (res.ret === 0) {\n                //发送登录请求\n                let param = new URLSearchParams();\n                param.append(\"username\", that.loginForm.username);\n                param.append(\"password\", that.loginForm.password);\n\n                request.post(\"/login\", param).then(data  => {\n                  if (data.flag) {\n                    if (data.data.manager==0){\n                      that.$message.error(\"该用户不是管理员\");\n                    }else {\n                      sessionStorage.setItem(\"user\",JSON.stringify(data.data))//将登陆信息储存到session域中\n                      that.$message.success(\"登录成功\");\n                      that.$router.push({ path: \"/\" });\n                    }\n\n                  } else {\n                    that.$message.error(data.message);\n                  }\n                });\n              }\n            }\n          );\n          // 显示验证码\n          captcha.show();\n        } else {\n          return false;\n        }\n      });\n    }\n  }\n};\n</script>\n<style scoped>\n.login-container {\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  right: 0;\n  left: 0;\n  background: url(../assets/photo/bg.png) center center /\n    cover no-repeat;\n}\n.login-card {\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  right: 0;\n  background: #fff;\n  padding: 170px 60px 180px;\n  width: 350px;\n}\n.login-title {\n  color: #303133;\n  font-weight: bold;\n  font-size: 1rem;\n}\n.login-form {\n  margin-top: 1.2rem;\n}\n.login-card button {\n  margin-top: 1rem;\n  width: 100%;\n}\n</style>\n"]}]}