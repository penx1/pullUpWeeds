,EAEe;AAFD,YAAA,KAAK,EAAC;AAEL,WAFf,EAAwB;8BACtB;AAAA,qBAAgF,CAAhF,YAAA,CAAgF,mBAAhF,EAAgF;AAAtE,gBAAA,IAAI,EAAC,UAAiE;4BAA7C,KAAA,CAAA,IAAA,CAAK,OAAwC;;yBAA7C,KAAA,CAAA,IAAA,CAAK,OAAL,GAAY,M;kBAAiC;AAA/B,gBAAA,KAAmB,EAAnB;AAAA,2BAAA;AAAA;AAA+B,eAAhF,E,IAAA,E;;AAAA,gB,cAAA,CAAgF,CAAhF;AAAA,a,CADsB;;;;AAAA,WAAxB,CAfe,CAFf;AAAA,S;;;;OADF,E;;AAAA,Q,kBAAA,CAqBU,EACV,mBAAA,CAGa,MAHb,EAAA,WAAA,EAGa,CAFL,YAAA,CAAyD,oBAAzD,EAAyD;AAA7C,QAAA,OAAK,EAAA,MAAA,CAAA,EAAA,CAAA,KAAA,MAAA,CAAA,EAAA,CAAA,GAAA,UAAA,MAAA;AAAA,iBAAE,KAAA,CAAA,aAAA,GAAa,KAAf;AAAA,SAAA;AAAwC,OAAzD,E;0BAA0C;AAAA,iBAAG,C,WAAA,CAAH;AAAA,S;;;;OAA1C,CAEK,EADL,YAAA,CAAuD,oBAAvD,EAAuD;AAA5C,QAAA,IAAI,EAAC,SAAuC;AAA5B,QAAA,OAAK,EAAE,QAAA,CAAA;AAAqB,OAAvD,E;0BAAwC;AAAA,iBAAG,C,WAAA,CAAH;AAAA,S;;;;OAAxC,E;;AAAA,Q,WAAA,CACK,CAHb,CADU,CArBV;AAAA,K;;;;GADF,E;;AAAA,I,cAAA,CA4BI,CA5CN,C","sourcesContent":["\r\n<template>\r\n\r\n  <div id=\"login\">\r\n    <h1>登陆</h1>\r\n    <el-form  :model=\"form\"  :rules=\"rules\" ref=\"form\">\r\n      <el-form-item  prop=\"username\">\r\n        <el-input prefix-icon=\"el-icon-user\" v-model=\"form.username\" style=\"width: 100%;\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item prop=\"password\">\r\n        <el-input prefix-icon=\"el-icon-lock\" type=\"password\" v-model=\"form.password\" show-password style=\"width: 100%;\"></el-input>\r\n      </el-form-item>\r\n      <el-button type=\"primary\" @click=\"loginBackstage\" style=\"width: 100%\">登陆后台</el-button>\r\n      <br>\r\n      <el-button type=\"success\" @click=\"login\" style=\"width: 100%\">用户登陆</el-button>\r\n      <br>\r\n      <el-button type=\"warning\" @click=\"dialogVisible = true\" style=\"width: 100%\">用户注册</el-button>\r\n    </el-form>\r\n\r\n    <el-dialog title=\"注册\" v-model=\"dialogVisible\" width=\"30%\">\r\n      <el-form  :model=\"form\" label-width=\"120px\" :rules=\"rules\">\r\n        <el-form-item  prop=\"username\" label=\"用户名\">\r\n          <el-input v-model=\"form.username\" style=\"width: 80%;\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item  prop=\"password\" label=\"密码\" >\r\n          <el-input type=\"password\" v-model=\"form.password\" style=\"width: 80%;\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"nickname\" label=\"昵称\">\r\n          <el-input v-model=\"form.nickName\" style=\"width: 80%;\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"age\" label=\"年龄\">\r\n          <el-input v-model=\"form.age\" style=\"width: 80%;\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"sex\" label=\"性别\">\r\n          <el-radio v-model=\"form.sex\" label=\"男\">男</el-radio>\r\n          <el-radio v-model=\"form.sex\" label=\"女\">女</el-radio>\r\n          <el-radio v-model=\"form.sex\" label=\"未知\">未知</el-radio>\r\n        </el-form-item>\r\n        <el-form-item label=\"地址\">\r\n          <el-input type=\"textarea\" v-model=\"form.address\" style=\"width: 80%;\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n              <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n              <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n            </span>\r\n    </el-dialog>\r\n  </div>\r\n\r\n\r\n</template>\r\n<style>\r\n\r\nbody{\r\n  width: 100%;\r\n  height: 100%;\r\n  font-family: 'Open Sans',sans-serif;\r\n  margin: 0;\r\n  /*background-color: #FFABCC;*/\r\n  background-color: #79BBFF;\r\n}\r\n#login{\r\n  position: absolute;\r\n  top: 40%;\r\n  left:50%;\r\n  margin: -150px 0 0 -150px;\r\n  width: 300px;\r\n  height: 300px;\r\n}\r\n#login h1{\r\n  color: #fff;\r\n  text-shadow:0 0 10px;\r\n  letter-spacing: 1px;\r\n  text-align: center;\r\n}\r\nh1{\r\n  font-size: 2em;\r\n  margin: 0.67em 0;\r\n}\r\n\r\n</style>\r\n<script>\r\nimport request from \"../utils/request\";\r\n\r\nexport default {\r\n  data(){\r\n    return{\r\n      dialogVisible : false,\r\n      form:{},\r\n      rules: {\r\n        username: [\r\n          { required: true, message: '请输入用户名', trigger: 'blur' },\r\n        ],\r\n        password: [\r\n          { required: true, message: '请输入密码', trigger: 'blur' },\r\n        ],\r\n        nickname: [\r\n          { required: true, message: '请输入昵称', trigger: 'blur' },\r\n        ],\r\n        sex: [\r\n          { required: true, message: '请输入性别', trigger: 'blur' },\r\n        ],\r\n        age: [\r\n          { required: true, message: '请输入年龄', trigger: 'blur' },\r\n        ],\r\n      },\r\n\r\n    }\r\n  },\r\n  methods:{\r\n    save: function () {\r\n\r\n        request.post(\"/user\",this.form).then(res=> {\r\n          console.log(res)\r\n          if (res.code === '0'){\r\n            this.$message({\r\n              type:\"success\",\r\n              message:\"新增成功\"\r\n            })\r\n          }else {\r\n            this.$message({\r\n              type:\"error\",\r\n              message:res.msg\r\n            })\r\n          }\r\n          this.dialogVisible=false\r\n        })\r\n\r\n\r\n\r\n\r\n    },\r\n    loginBackstage(){\r\n      this.$refs[\"form\"].validate((valid) => {\r\n        if (valid) {\r\n          request.post('/user/login',this.form).then(res => {\r\n            if (res.code === '0'){\r\n              this.$message({\r\n                type:\"success\",\r\n                message:\"登陆成功\"\r\n              })\r\n              sessionStorage.setItem(\"user\",JSON.stringify(res.data))//将登陆信息储存到session域中\r\n              this.$router.push(\"/\")//登陆成功重定向到主页\r\n        }else {\r\n              this.$message({\r\n                type: \"error\",\r\n                message: res.msg\r\n              })\r\n            }\r\n          })\r\n\r\n        }\r\n      })\r\n\r\n    },\r\n\r\n    login(){\r\n      this.$refs[\"form\"].validate((valid) => {\r\n        if (valid) {\r\n          request.post('/user/login',this.form).then(res => {\r\n            if (res.code === '0'){\r\n              this.$message({\r\n                type:\"success\",\r\n                message:\"登陆成功\"\r\n              })\r\n              sessionStorage.setItem(\"user\",JSON.stringify(res.data))//将登陆信息储存到session域中\r\n              this.$router.push(\"/store\")//登陆成功重定向到主页\r\n            }else {\r\n              this.$message({\r\n                type: \"error\",\r\n                message: res.msg\r\n              })\r\n            }\r\n          })\r\n\r\n        }\r\n      })\r\n\r\n    }\r\n  },\r\n\r\n  name: \"Login\"\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":""}]}