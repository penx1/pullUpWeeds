AAA;AAAA,iBAAE,KAAA,CAAA,aAAA,GAAa,KAAf;AAAA,SAAA;AAAwC,OAAxE,E;0BAAyD;AAAA,iBAAG,C,WAAA,CAAH;AAAA,S;;;;OAAzD,CACK,CAHC,CAP4B,CAA5B;AAAA,K;;;;GAHF,E;;AAAA,I,8BAAA,CA4BI,EAZJ,YAAA,CAUY,oBAVZ,EAUY;gBATC,KAAA,CAAA,UASD;;aATC,KAAA,CAAA,UAAA,GAAU,M;MASX;AATa,IAAA,KAAK,EAAC,KASnB;AAT0B,oBAAc,QAAA,CAAA;AASxC,GAVZ,E;sBAGE;AAAA,aAA6B,CAA7B,WAA6B,EAAA,WAAA,EAE7B,WAF6B,EAG7B,mBAAA,CAGD,MAHC,EAAA,WAAA,EAGD,CADL,YAAA,CAAyD,oBAAzD,EAAyD;AAA9C,QAAA,IAAI,EAAC,SAAyC;AAA9B,QAAA,OAAK,EAAE,QAAA,CAAA;AAAuB,OAAzD,E;0BAA0C;AAAA,iBAAG,C,WAAA,CAAH;AAAA,S;;;;OAA1C,E;;AAAA,Q,WAAA,CACK,CAHC,CAH6B,CAA7B;AAAA,K;;;;GAHF,E;;AAAA,I,8BAAA,CAYI,CAxCN,CA2CI,CA7EN,CA+EI,CAhFN,C","sourcesContent":["<template>\r\n  <div >\r\n    <div style=\"padding: 10px\">\r\n      <div style=\"margin: 10px 0\">\r\n\r\n        <el-button type=\"success\"  @click=\"dialogVisible = true\">立即购买</el-button>总价：{{pri}} 元\r\n      </div>\r\n      <div  style=\"margin: 10px 0\">\r\n        <el-input v-model=\"search\" placeholder=\"输入关键字\" style=\"width: 20%\" clearable></el-input>\r\n        <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"load\">查询</el-button>\r\n      </div>\r\n      <el-table :data=\"tableData\" border  stripe style=\"width: 100%\">\r\n        <el-table-column prop=\"id\" label=\"ID\" sortable=\"\"> </el-table-column>\r\n        <el-table-column prop=\"name\" label=\"商品名\" sortable=\"\"> </el-table-column>\r\n        <el-table-column prop=\"price\" label=\"单价\" sortable=\"\"></el-table-column>\r\n        <el-table-column prop=\"author\" label=\"供应商\" sortable=\"\"></el-table-column>\r\n        <el-table-column prop=\"createTime\" label=\"上架时间\" sortable=\"\"></el-table-column>\r\n        <el-table-column  label=\"产品图片\" >\r\n          <template  #default=\"scope\">\r\n            <el-image\r\n                style=\"width: 100px; height: 100px\"\r\n                :src=\"scope.row.cover\"\r\n                :preview-src-list=\"scope.row.cover\"\r\n            ></el-image>\r\n          </template>\r\n\r\n          </el-table-column>\r\n        <el-table-column fixed=\"right\"   label=\"购买数量\">\r\n          <template  #default=\"scope\">\r\n            <el-input-number v-model=\"scope.row.num\" @change=\"price\" :min=\"1\" :max=\"10\" label=\"描述文字\"></el-input-number>\r\n\r\n          </template>\r\n      </el-table-column>\r\n\r\n\r\n      </el-table>\r\n      <div style=\"margin: 10px\">\r\n        <el-pagination\r\n            @size-change=\"handleSizeChange\"\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"currentPage\"\r\n            :page-sizes=\"[5,10,20]\"\r\n            :page-size=\"pageSize\"\r\n            layout=\"total, sizes, prev, pager, next, jumper\"\r\n            :total=\"total\"\r\n        >\r\n        </el-pagination>\r\n\r\n        <el-dialog\r\n            v-model=\"dialogVisible\" width=\"30%\" :before-close=\"handleClose\"\r\n            >\r\n          <span>     支付{{pri}}元</span><br>\r\n\r\n           <el-image\r\n            style=\"width: 400px; height: 400px\"\r\n            :src=\"url\"\r\n            :fit=\"fits\"></el-image>\r\n          <br>\r\n          <span slot=\"footer\" class=\"dialog-footer\">\r\n    <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n    <el-button type=\"primary\" @click=\"dialogVisible = false\">确 定</el-button>\r\n  </span>\r\n        </el-dialog>\r\n\r\n        <el-dialog\r\n            v-model=\"eroVisible\" width=\"30%\" :before-close=\"handleClose\"\r\n        >\r\n          <span>   购物车无商品，请添加商品。</span><br>\r\n\r\n          <br>\r\n          <span slot=\"footer\" class=\"dialog-footer\">\r\n\r\n    <el-button type=\"primary\" @click= toStore>确 定</el-button>\r\n  </span>\r\n        </el-dialog>\r\n\r\n      </div>\r\n\r\n\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// @ is an alias to /src\r\n\r\n\r\nimport request from \"../utils/request\";\r\n\r\nexport default {\r\n  data() {\r\n    return{\r\n\r\n      fits: ['fill', 'contain', 'cover', 'none', 'scale-down'],\r\n      url: require('../er.jpg'),\r\n      pri:0,\r\n      money:0,\r\n      num:0,\r\n      currentPage:1,\r\n      pageSize: 10,\r\n      eroVisible:false,\r\n      dialogVisible: false,\r\n      form: {\r\n        name: 233\r\n      },\r\n      total:0,\r\n\r\n      search: '',\r\n      tableData: [\r\n\r\n      ]\r\n    }\r\n  },\r\n  name: 'Cart',\r\n  components: {\r\n\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n\r\n  methods:{\r\n    toStore(){\r\n      // this.$router.go(-1);\r\n      this.$router.replace(\"/store\")\r\n    },\r\n    handleClose(done) {\r\n      this.$confirm('是否退出')\r\n          .then(_ => {\r\n            done();\r\n          })\r\n          .catch(_ => {});\r\n    },\r\n    price(){\r\n      let n = 0\r\n      this.tableData.filter((data)=>{\r\n\r\n        if (data.num!=null){\r\n          n=data.num*data.price+n\r\n          console.log(n)\r\n        }\r\n      })\r\n\r\n      this.pri=n\r\n    },\r\n    filesUploadSuccess(res){\r\n      this.form.cover=res.data\r\n    },\r\n    load(){\r\n      let goodss =sessionStorage.getItem('goods')\r\n      console.log(goodss)\r\n\r\n\r\n      if (goodss !== null){\r\n        goodss = goodss.slice(1,goodss.length-1)\r\n        request.get(\"/store/cart\",{\r\n          // get请求需要params\r\n          params:{\r\n            pageNum: this.currentPage,\r\n            pageSize: this.pageSize,\r\n            search: this.search,\r\n            goods:goodss\r\n          }\r\n        }).then(res =>{\r\n          this.tableData = res.data.records\r\n          this.total = res.data.total\r\n        })\r\n\r\n      }else {\r\n        this.eroVisible = true\r\n      }\r\n\r\n    },\r\n\r\n    add(){\r\n      this.dialogVisible = true\r\n      this.form={}\r\n      this.$refs['upload'].clearFiles()\r\n    },\r\n    save: function () {\r\n      if (this.form.id){//  更新\r\n        request.put(\"/store\",this.form).then(res=> {\r\n          console.log(res)\r\n          if (res.code === '0'){\r\n            this.$message({\r\n              type:\"success\",\r\n              message:\"更新成功\"\r\n            })\r\n          }else {\r\n            this.$message({\r\n              type:\"error\",\r\n              message:res.msg\r\n            })\r\n          }\r\n          this.load()\r\n          this.dialogVisible=false\r\n        })\r\n        this.dialogVisible = false\r\n      }else {//  新增\r\n        request.post(\"/store\",this.form).then(res=> {\r\n          console.log(res)\r\n          if (res.code === '0'){\r\n            this.$message({\r\n              type:\"success\",\r\n              message:\"新增成功\"\r\n            })\r\n          }else {\r\n            this.$message({\r\n              type:\"error\",\r\n              message:res.msg\r\n            })\r\n          }\r\n          this.load()\r\n          this.dialogVisible=false\r\n        })\r\n\r\n      }\r\n\r\n\r\n    },\r\n    handleEdit(row){\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogVisible = true\r\n    },\r\n    handleDelete(id){\r\n      request.delete(\"/store/\"+id).then(res=>{\r\n        if (res.code === '0'){\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"删除成功\"\r\n          })\r\n        }else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n        this.load()\r\n      })\r\n    },\r\n    handleSizeChange(pageSize){//改变当前页数触发\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum){//改变当前页码触发\r\n      this.currentPage = pageNum\r\n      this.load()\r\n    },\r\n\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n"],"sourceRoot":""}]}