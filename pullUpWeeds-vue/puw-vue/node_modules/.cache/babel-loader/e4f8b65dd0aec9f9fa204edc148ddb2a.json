AAI,GAAG,CAAC,GAAJ,KAAY,CAAhB,EAAmB;AACjB;AACA,kBAAI,KAAI,GAAI,IAAI,eAAJ,EAAZ;AACA,cAAA,KAAK,CAAC,MAAN,CAAa,UAAb,EAAyB,IAAI,CAAC,SAAL,CAAe,QAAxC;AACA,cAAA,KAAK,CAAC,MAAN,CAAa,UAAb,EAAyB,IAAI,CAAC,SAAL,CAAe,QAAxC;AAEA,cAAA,OAAO,CAAC,IAAR,CAAa,QAAb,EAAuB,KAAvB,EAA8B,IAA9B,CAAmC,UAAA,IAAG,EAAM;AAC1C,oBAAI,IAAI,CAAC,IAAT,EAAe;AAEX,kBAAA,cAAc,CAAC,OAAf,CAAuB,MAAvB,EAA8B,IAAI,CAAC,SAAL,CAAe,IAAI,CAAC,IAApB,CAA9B,EAFW,CAE6C;;AACxD,kBAAA,IAAI,CAAC,QAAL,CAAc,OAAd,CAAsB,MAAtB;AACA,kBAAA,IAAI,CAAC,OAAL,CAAa,IAAb,CAAkB;AAAE,oBAAA,IAAI,EAAE;AAAR,mBAAlB;AAEJ,iBANA,MAMO;AACL,kBAAA,IAAI,CAAC,QAAL,CAAc,KAAd,CAAoB,IAAI,CAAC,OAAzB;AACF;AACD,eAVD;AAWF;AACF,WAtBY,CAAd,CAJS,CA4BT;;AACA,UAAA,OAAO,CAAC,IAAR;AACF,SA9BA,MA8BO;AACL,iBAAO,KAAP;AACF;AACD,OAlCD;AAmCF;AAxCO;AAfI,CAAf","sourcesContent":["<template>\n  <div class=\"login-container\">\n    <div class=\"login-card\">\n      <div class=\"login-title\">TureHouse用户登录</div>\n      <!-- 登录表单 -->\n      <el-form\n        status-icon\n        :model=\"loginForm\"\n        :rules=\"rules\"\n        ref=\"ruleForm\"\n        class=\"login-form\"\n      >\n        <!-- 用户名输入框 -->\n        <el-form-item prop=\"username\">\n          <el-input\n            v-model=\"loginForm.username\"\n            prefix-icon=\"el-icon-user-solid\"\n            placeholder=\"用户名\"\n            @keyup.enter.native=\"login\"\n          />\n        </el-form-item>\n        <!-- 密码输入框 -->\n        <el-form-item prop=\"password\">\n          <el-input\n            v-model=\"loginForm.password\"\n            prefix-icon=\"el-icon-lock\"\n            show-password\n            placeholder=\"密码\"\n            @keyup.enter.native=\"login\"\n          />\n        </el-form-item>\n      </el-form>\n      <!-- 登录按钮 -->\n      <div><el-button type=\"primary\" @click=\"login\">登录</el-button></div>\n      <div>\n        <el-button type=\"success\" @click=\"register\">去注册</el-button>\n      </div>\n\n    </div>\n  </div>\n</template>\n<script>\nimport request from \"@/utils/request\";\nimport axios from \"axios\";\n\nexport default {\n  data: function() {\n    return {\n      loginForm: {\n        username: \"\",\n        password: \"\"\n      },\n      rules: {\n        username: [\n          { required: true, message: \"用户名不能为空\", trigger: \"blur\" }\n        ],\n        password: [{ required: true, message: \"密码不能为空\", trigger: \"blur\" }]\n      }\n    };\n  },\n  methods: {\n    register(){\n      this.$router.push(\"/register\")\n    },\n    login() {\n      this.$refs.ruleForm.validate(valid => {\n        if (valid) {\n          const that = this;\n          // eslint-disable-next-line no-undef\n\n          var captcha = new   TencentCaptcha(\n            // this.config.TENCENT_CAPTCHA,2088053498\n              \"2068422883\",\n            function(res) {\n              if (res.ret === 0) {\n                //发送登录请求\n                let param = new URLSearchParams();\n                param.append(\"username\", that.loginForm.username);\n                param.append(\"password\", that.loginForm.password);\n\n                request.post(\"/login\", param).then(data  => {\n                  if (data.flag) {\n\n                      sessionStorage.setItem(\"user\",JSON.stringify(data.data))//将登陆信息储存到session域中\n                      that.$message.success(\"登录成功\");\n                      that.$router.push({ path: \"/\" });\n\n                  } else {\n                    that.$message.error(data.message);\n                  }\n                });\n              }\n            }\n          );\n          // 显示验证码\n          captcha.show();\n        } else {\n          return false;\n        }\n      });\n    }\n  }\n};\n</script>\n<style scoped>\n.login-container {\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  right: 0;\n  left: 0;\n  background: url(../assets/photo/bg.png) center center /\n    cover no-repeat;\n}\n.login-card {\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  right: 0;\n  background: #fff;\n  padding: 170px 60px 180px;\n  width: 350px;\n}\n.login-title {\n  color: #303133;\n  font-weight: bold;\n  font-size: 1rem;\n}\n.login-form {\n  margin-top: 1.2rem;\n}\n.login-card button {\n  margin-top: 1rem;\n  width: 100%;\n}\n</style>\n"],"sourceRoot":""}]}